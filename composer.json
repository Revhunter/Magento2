{
  "name": "revhunter/magento2",
  "description": "Magento 2 module - Revhunter",
  "type": "magento2-module",
  "version": "1.0.0",
  "license": [
    "OSL-3.0"
  ],
  "require": {
    "php": "^7.1",
    "magento/magento-composer-installer": "*",
    "magento/module-catalog": "*",
    "magento/module-cms": "*",
    "magento/module-checkout": "*",
    "magento/module-store": "*",
    "magento/framework": "*"
  },
  "autoload": {
    "files": [
      "src/registration.php"
    ],
    "psr-4": {
      "Fwc\\RevHunter\\": "src"
    }
  },
  "keywords": [
    "magento2",
    "rev",
    "hunter",
    "revhunter"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
